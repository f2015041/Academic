#include <stdio.h>

void
main ()
{
  int a[100], b[100], n, i, j, flag = 1;
  printf ("Input n");
  scanf ("%d", &n);
  printf ("Input arr1");
  for (i = 0; i < n; i++)
    {
      scanf ("%d", &a[i]);
    }
  printf ("Input arr2");
  for (i = 0; i < n; i++)
    {
      scanf (" % d ", &b[i]);
    }

  printf (" Loop checking ");
  for (i = 0; i < n; i++)
    {
      if (a[i] != b[j])
	flag = 0;
    }

  if (flag)
    printf (" Same ");
}
